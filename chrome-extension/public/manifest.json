{
  "manifest_version": 2,
  "name": "Ctrl V",
  "description": "Send links, texts and files between your devices.",
  "version": "1.0",
  "icons": {
    "16": "icons/v16.png",
    "48": "icons/v48.png",
    "128": "icons/v128.png"
  },
  "permissions": ["activeTab", "tabs"],
  "browser_action": {
    "default_popup": "index.html"
  }
}
