{
  "manifest_version": 2,
  "name": "Ctrl V",
  "description": "Send anything to your mobile device",
  "version": "1.0",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": ["activeTab", "tabs"],
  "browser_action": {
    "default_popup": "index.html"
  }
}
